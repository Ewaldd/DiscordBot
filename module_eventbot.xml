    <path refid="library.maven:_org.jetbrains.kotlin:kotlin-stdlib-jdk7:1.2.30.classpath"/>
  </path>
  
  <path id="eventbot.module.classpath">
    <path refid="${module.jdk.classpath.eventbot}"/>
    <pathelement location="${eventbot.output.dir}"/>
    <path refid="library.maven:_net.dv8tion:jda:3.5.0_329.classpath"/>
