  </path>
  
  
  <patternset id="excluded.from.module.eventbot">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.eventbot">
    <patternset refid="excluded.from.module.eventbot"/>
  </patternset>
  
  <path id="eventbot.module.sourcepath">
    <dirset dir="${module.eventbot.basedir}">
      <include name="src/main/kotlin"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.eventbot" depends="compile.module.eventbot.production,compile.module.eventbot.tests" description="Compile module EventBot"/>
  
  <target name="compile.module.eventbot.production" depends="register.custom.compilers" description="Compile module EventBot; production classes">
    <mkdir dir="${eventbot.output.dir}"/>
    <javac2 destdir="${eventbot.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.eventbot}/javac">
      <compilerarg line="${compiler.args.eventbot}"/>
      <bootclasspath refid="eventbot.module.bootclasspath"/>
      <classpath refid="eventbot.module.production.classpath"/>
      <src refid="eventbot.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.eventbot"/>
    </javac2>
    
    <copy todir="${eventbot.output.dir}">
      <fileset dir="${module.eventbot.basedir}/src/main/kotlin">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.eventbot.tests" depends="register.custom.compilers,compile.module.eventbot.production" description="compile module EventBot; test classes" unless="skip.tests"/>
  
  <target name="clean.module.eventbot" description="cleanup module">
    <delete dir="${eventbot.output.dir}"/>
    <delete dir="${eventbot.testoutput.dir}"/>
  </target>
</project>