  <property name="compiler.args.eventbot" value="-encoding UTF-8 -source 1.8 -target 1.8 ${compiler.args}"/>
  
  <property name="eventbot.output.dir" value="${module.eventbot.basedir}/target/classes"/>
  <property name="eventbot.testoutput.dir" value="${module.eventbot.basedir}/target/test-classes"/>
  
