    <path refid="library.maven:_org.jetbrains.kotlin:kotlin-test:1.2.30.classpath"/>
  </path>
  
  <path id="eventbot.runtime.module.classpath">
    <pathelement location="${eventbot.testoutput.dir}"/>
    <pathelement location="${eventbot.output.dir}"/>
    <path refid="library.maven:_net.dv8tion:jda:3.5.0_329.classpath"/>
    <path refid="library.maven:_com.google.code.findbugs:jsr305:3.0.2.classpath"/>
